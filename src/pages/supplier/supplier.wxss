@import '../../components/dialog/carSourceDetail/carSourceDetail.wxss';
.supplier {
}

.supplier .header {
  width: 100%;
  height: 180rpx;
  background: #ECF0F7;
}

.supplier .body {
  width: 100%;
  height: auto;
}

.supplier .section {
  width: 100%;
  height: 78rpx;
}

.supplier .rows {

}

.supplier .row-1 {
  position: relative;
  height: 120rpx;
  width: 100%;
  box-sizing: border-box;
  background: #ffffff;
  padding: 0rpx 30rpx;
}

.supplier .row-1::after {
  content:'';
  position:absolute;
  left:30rpx;
  right:30rpx;
  bottom:0;
  border-bottom:1rpx solid #CCCCCC
}

.supplier .row-2 {
  position: relative;
  width: 100%;
  height: 88rpx;
  box-sizing: border-box;
  background: #ffffff;
  padding-left: 30rpx;
}

.supplier .row-2::after {
  content:'';
  position:absolute;
  left:30rpx;
  right:0;
  bottom:0;
  border-bottom:1rpx solid #E4E4E4
}

/* 顶部搜索栏 */
.search-bar {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  padding: 0rpx 50rpx;
}

.search-bar .search-input {
  position: relative;
  height: 90rpx;
  width: 100%;
}

.search-bar .search-input input {
  background: #FFFFFF;
  box-shadow: 0 3rpx 10rpx 0 rgba(50,57,68,0.30);
  border-radius: 6rpx;
  height: 100%;
  width: 100%;
}

.search-bar .search-input .placeholder {
  font-family: "PingFangSC-Regular";
  font-size: 30rpx;
  color: #AAAAAA;
  letter-spacing: 0;
  line-height: 28rpx;
  text-align: center;
}

.search-bar .search-input .find {
  position: absolute;
  top: 0rpx;
  left: 0rpx;
  width: 24rpx;
  height: 24rpx;
}

.search-bar .search-input .close {
  position: absolute;
  top: 0rpx;
  right: 0rpx;
  width: 24rpx;
  height: 24rpx;
}

/* 供应商分区 */
.section-board {
  background: #ffffff;
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 100%;
}

.section-board .title {
  font-family: "PingFangSC-Regular";
  font-size: 28rpx;
  color: #666666;
  letter-spacing: 0;
  line-height: 28rpx;
  text-align: center;
  margin: 0rpx 30rpx;
}

.section-board .line {
  height: 1rpx;
  width: 60rpx;
  background: #7A6D6E;
}

/* 模糊供应商行 */
.supplier-fuzzy-search-result {
  position: relative;
  height: 100%;
  width:100%;
  line-height:88rpx;
  font-family:"PingFangSC-Regular";
  font-size:30rpx;
  color:#333333;
  background: #ffffff;
}

/* 精准供应商行 */
.supplier-search-result {
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 100%;
  height: 100%;
  background: #ffffff;
}

.supplier-search-result .profile {
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-family: "PingFangSC-Medium";
  font-size: 30rpx;
  color: #333333;
  letter-spacing: 0rpx;
  box-sizing: border-box;
}

.supplier-search-result .profile .tag-label {
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-family: "PingFangSC-Regular";
  font-size: 24rpx;
  color: #3377EE;
  letter-spacing: 0;
}

.supplier-search-result .profile .tag-label image{
  margin-left: 20rpx;
  margin-right: 6rpx;
  width: 24rpx;
  height: 24rpx;
}

.supplier-search-result .call-line {
  padding: 40rpx 20rpx;
  width: 40rpx;
  height: 40rpx
}
