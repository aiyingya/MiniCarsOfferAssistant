@import '../../components/supplier/index.wxss';
@import '../../components/supplierDetail/index.wxss';
@import '../../components/dialog/carSourceDetail/carSourceDetail.wxss';
.supplier {}

.supplier .header {
  width: 100%;
  height: 180rpx;
  background: #4D5767;
}

.supplier .body {
  width: 100%;
  height: auto;
}

.supplier .section {
  width: 100%;
  height: 78rpx;
}

.supplier .rows {}

.supplier .row-1 {
  position: relative;
  max-height: 168rpx;
  width: 100%;
  box-sizing: border-box;
  background: #ffffff;
  padding: 0rpx 30rpx 10rpx 30rpx;
}

.supplier .row-1 .filter{
  padding-left: 20rpx;
  background: #FAFAFA;
}


.supplier .row-2 {
  position: relative;
  width: 100%;
  height: 88rpx;
  box-sizing: border-box;
  background: #ffffff;
  padding-left: 30rpx;
}

.supplier .row-2::after {
  content: '';
  position: absolute;
  left: 30rpx;
  right: 0;
  bottom: 0;
  border-bottom: 1rpx solid #E4E4E4
}

