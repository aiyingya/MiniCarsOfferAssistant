.page{
    background-color: #EFEFEF;
}
.quotation-list{
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  width: 100%;
  margin:0;
  transition: all 0.2s ease-in-out;
  z-index: 9;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  overflow: hidden;
}
.quotation-item-hd{
  margin-top: 38rpx;
  padding: 0 30rpx;
  height: 36rpx;
  line-height: 36rpx;
}
.quotation-price{
    font-size: 30rpx;
    color: #333;
}
.quotation-title{
    font-weight: 700;
    padding-right: 20px;
    font-size: 30rpx;
}
.red-price{
    color: #ED4149;
    font-size: 44rpx;
    display: inline;
    letter-spacing: 0;
    line-height: 40rpx;
}
.quotation-msg{
  line-height: 26rpx;
  height: 26rpx;
  font-size: 26rpx;
  margin: 20rpx 0 40rpx 0;
  padding: 0 30rpx;
  color: #666666;
  display: -webkit-flex;
  display: flex;
  -webkit-justify-content: flex-start;
  justify-content: flex-start;
  align-items: center;
}

.quotation-submsg{
  height: 88rpx;
  line-height: 88rpx;
  padding: 0 30rpx;
}

.line-box{
  position: relative;
  margin-right: 5px;
  font-size: 24rpx;
  color: #999;
}
.icon-line {
  padding-left: 20rpx;
}
.text-right{
    text-align: right;
}
.min-font-gray{
    color: #666;
    font-size: 24rpx;
}

.orderlist-noorder-image{
    width: 160rpx;
    height: 160rpx;
    margin: 0 auto;
    display: block;
}

.orderlist-noorder-text{
    font-size: 28rpx;
    color: #999999;
    letter-spacing: -0.56rpx;
    line-height: 28rpx;
    text-align: center;
}

.noorder{
    position: absolute;
    width: 200rpx;
    heigh: 200rpx;
    top: 50%;
    left: 50%;
    margin: -100rpx 0 0 -100rpx;
}

.depreciate-wrap {
    display: flex;
}

.depreciate-icon {
  position: absolute;
  left: 0;
  top: 50%;
  margin-top: -12rpx;
  width: 20rpx;
  height: 24rpx;
}
.record-cell {
  position: relative;
  margin-bottom: 10rpx;
  width: 100%;
  height: 248rpx;
  overflow: hidden;
  background-color: #fff;
}
.cancel-attention {
	position: absolute;
	top: 0;
	right: 0;
  bottom:0;
	width: 150rpx;
	background: #ED4149;
	font-size: 32rpx;
	color: #FFFFFF;
	text-align: center;
	line-height: 248rpx;
	z-index: 1;
}
.weui-cells:after,
.weui-cells:before {
  display: none;
}