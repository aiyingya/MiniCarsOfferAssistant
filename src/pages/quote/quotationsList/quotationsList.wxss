.page{
    background-color: #EFEFEF;
}
.users-cells {
  width: 100%;
  height: auto;
  position: relative;
}
.users-cell {
  margin-bottom: 20rpx;
}
.users-cell_hd {
  padding: 20rpx 30rpx;
  background-color: #F6F7F8;
}
.users-cell_hd .title {
  font-family: "PingFangSC-Regular";
  font-size: 30rpx;
  color: #333333;
  line-height: 36rpx;
}
.users-cell_hd .messages {
  display: -webkit-flex;
  display: flex;
  -webkit-justify-content: space-between;
  justify-content: space-between;
  -webkit-align-items: center;
  align-items: center;
  font-family: "PingFangSC-Regular";
  font-size: 24rpx;
  height: 60rpx;
  line-height: 60rpx;
}
.users-cell_bd {
  background-color: #fff;
}
.cars-cell {
  position: relative;
  padding: 20rpx 30rpx;
}
.cars-cell:after {
  content: '';
  position: absolute;
  left: 30rpx;
  right: 0;
  bottom: 0;
  border-bottom: 1rpx solid #E5E5E5;
}
.cars-cell:last-child:after {
  display: none;
}
.setup-time {
  height: 60rpx;
  line-height: 60rpx;
  font-family: "PingFangSC-Regular";
  font-size: 26rpx;
  color: #666666;
  text-align: center;
}
.cars-name {
  font-family: "PingFangSC-Medium";
  font-size: 30rpx;
  color: #333333;
  line-height: 36rpx;
}
.cars-message {
  display: -webkit-flex;
  display: flex;
  -webkit-justify-content: space-between;
  justify-content: space-between;
  -webkit-align-items: baseline;
  align-items: baseline;
}

.price {
  display: inline;
  font-family: ".SFNSDisplay";
  font-size: 44rpx;
  color: #ED4149;
  letter-spacing: 0;
  line-height: 40rpx; 
}
.check-more {
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-family: "PingFangSC-Regular";
  font-size: 26rpx;
  color: #333333;
  background-color: #fff;
}
.check-number {
  color: #475593;
}
.check-times,
.check-address {
  position: relative;
  padding-left: 25rpx;
  height: 60rpx;
}
.check-times .icon,
.check-address .icon {
  width: 20rpx;
  height: 20rpx;
  position: absolute;
  left: 0;
  top: 50%;
  margin-top: -10rpx;
}
.quotation-list{
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  width: 100%;
  margin:0;
  transition: all 0.2s ease-in-out;
  z-index: 9;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  overflow: hidden;
}
.quotation-item-hd{
  margin-top: 38rpx;
  padding: 0 30rpx;
  height: 36rpx;
  line-height: 36rpx;
}
.quotation-price{
    font-size: 30rpx;
    color: #333;
}
.quotation-title{
    font-weight: 700;
    padding-right: 20px;
    font-size: 30rpx;
}
.red-price{
    color: #ED4149;
    font-size: 44rpx;
    display: inline;
    letter-spacing: 0;
    line-height: 40rpx;
}
.quotation-msg{
  line-height: 26rpx;
  height: 26rpx;
  font-size: 26rpx;
  margin: 20rpx 0 0 0;
  color: #666666;
  display: -webkit-flex;
  display: flex;
  -webkit-justify-content: flex-start;
  justify-content: flex-start;
  align-items: center;
}

.quotation-submsg{
  height: 88rpx;
  line-height: 88rpx;
  padding: 0 30rpx;
}

.line-box{
  position: relative;
  margin-right: 5px;
  font-size: 24rpx;
  color: #999;
}
.icon-line {
  padding-left: 20rpx;
}
.text-right{
    text-align: right;
}
.min-font-gray{
    color: #666;
    font-size: 24rpx;
}

.orderlist-noorder-image{
    width: 160rpx;
    height: 160rpx;
    margin: 0 auto;
    display: block;
}

.orderlist-noorder-text{
    font-size: 28rpx;
    color: #999999;
    letter-spacing: -0.56rpx;
    line-height: 28rpx;
    text-align: center;
}

.noorder{
    position: absolute;
    width: 200rpx;
    heigh: 200rpx;
    top: 50%;
    left: 50%;
    margin: -100rpx 0 0 -100rpx;
}

.depreciate-wrap {
    display: flex;
}

.depreciate-icon {
  position: absolute;
  left: 0;
  top: 50%;
  margin-top: -12rpx;
  width: 20rpx;
  height: 24rpx;
}
.record-cell {
  position: relative;
  margin-bottom: 10rpx;
  width: 100%;
  height: 248rpx;
  overflow: hidden;
  background-color: #fff;
}
.cancel-attention {
	position: absolute;
	top: 0;
	right: 0;
  bottom:0;
	width: 150rpx;
	background: #ED4149;
	font-size: 32rpx;
	color: #FFFFFF;
	text-align: center;
	line-height: 248rpx;
	z-index: 1;
}
.weui-cells:after,
.weui-cells:before {
  display: none;
}