page {
  height: 100%;
}
.container {
  padding-bottom: 108rpx;
  box-sizing: border-box;
}
.container.height100 {
  height: 100%;
}
.users-cell {
  height: 150px;
  background-color: #fff;
  box-sizing: border-box;
  padding: 15rpx 30rpx;
}
.users-title {
  margin-top: 20rpx;
  font-family: "PingFangSC-Regular";
  font-size: 30rpx;
  color: #333333;
  line-height: 30rpx;
  text-align: center;
}
.users-cell .check-time {
  margin-top: 20rpx;
  font-family: "PingFangSC-Regular";
  font-size: 26rpx;
  color: #666666;
  line-height: 26rpx;
  text-align: center;
}
.cars-name {
  margin-top: 20rpx;
  font-family: "PingFangSC-Medium";
  font-size: 30rpx;
  color: #333333;
  line-height: 36rpx;
}
.cars-message {
  display: -webkit-flex;
  display: flex;
  -webkit-justify-content: space-between;
  justify-content: space-between;
  -webkit-align-items: baseline;
  align-items: baseline;
}

.price {
  display: inline;
  font-family: ".SFNSDisplay";
  font-size: 44rpx;
  color: #ED4149;
  letter-spacing: 0;
  line-height: 40rpx; 
}
.quotation-msg{
  line-height: 26rpx;
  height: 26rpx;
  font-size: 26rpx;
  margin: 20rpx 0 0 0;
  color: #666666;
  display: -webkit-flex;
  display: flex;
  -webkit-justify-content: flex-start;
  justify-content: flex-start;
  align-items: center;
}

.line-box{
  position: relative;
  margin-right: 5px;
  font-size: 24rpx;
  color: #999;
}
.icon-line {
  padding-left: 20rpx;
}
.text-right{
    text-align: right;
}
.min-font-gray{
    color: #666;
    font-size: 24rpx;
}
.depreciate-icon {
  position: absolute;
  left: 0;
  top: 50%;
  margin-top: -12rpx;
  width: 20rpx;
  height: 24rpx;
}
.history-wrap {
  width: 100%;
  margin-top: 20rpx;
  background-color: #fff;
}
.history-wrap.nohistory {
  height: calc(100% - 150px);
}
.history-wrap .title {
  position: relative;
  height: 90rpx;
  line-height: 90rpx;
  padding: 0 30rpx;
  font-family: "PingFangSC-Regular";
  font-size: 28rpx;
  color: #333333;
  background-color: #fff;
}
.history-wrap .title:after {
  content: '';
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  border-bottom: 1rpx solid #E4E4E4;
}
.history-cells {
  width: 100%;
  height: auto;
}
.history-cell:nth-of-type(odd) {
  background-color: #FAFAFA;
}
.history-message {
  display: -webkit-flex;
  display: flex;
  -webkit-justify-content: flex-start;
  justify-content: flex-start;
  -webkit-align-items: center;
  align-items: center;
  height: 100rpx;
  line-height: 100rpx;
  padding: 0 30rpx;
}
.history-message .check-time,
.history-message .check-address {
  width: 49%;
  margin-right: 1%;
  position: relative;
  padding-left: 25rpx;
  height: 100rpx;
}
.history-message .check-address {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.check-time .icon,
.check-address .icon {
  width: 20rpx;
  height: 20rpx;
  position: absolute;
  left: 0;
  top: 50%;
  margin-top: -8rpx;
}
.history-amap {
  width: 100%;
  height: 300rpx;
  position: relative;
}
.map_container{
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
}
.map{
  width: 100%;
  height: 100%;
}
.btn-wrap {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 98rpx;
  background-color: #fff;
  display: -webkit-flex;
  display: flex;
  -webkit-justify-content: flex-start;
  justify-content: flex-start;
}
.btn-wrap:after {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  border-top: 1rpx solid #ccc;
}
.btn-wrap .btn {
  flex-grow: 1;
  height: 98rpx;
  line-height: 98rpx;
  text-align: center;
  font-family: "PingFangSC-Regular";
  font-size: 28rpx;
  color: #333333;
}
.btn-wrap .contact-btn {
  background-color: #ED4149;
  color: #fff;
}
.nodata image {
  display: block;
  width: 100rpx;
  height: 100rpx;
  margin: 20rpx auto;
}
.nodata .text {
  line-height: normal;
  text-align: center;
  color: #666666;
}
.nodata .text.share {
  text-decoration: underline;
  color: #333;
}