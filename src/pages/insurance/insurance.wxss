.insurance-wrapper {
  overflow: hidden;
  padding-bottom: 118rpx;
}
.title {
  padding: 0 30rpx;
  height: 88rpx;
  line-height: 88rpx;
  background-color: #EFEFEF;
  display: -webkit-flex;
  display: flex;
  -webkit-justify-content: space-between;
  justify-content: space-between;
  font-family: "PingFangSC-Regular";
}
.title .label {
  font-size: 28rpx;
  color: #888888;
}
.title .value {
  font-size: 32rpx;
  color: #ED4149;
}
.insurance-wrapper .weui-cells:after,
.insurance-wrapper .weui-cells:before {
  border: none;
}
.weui-cell {
  padding: 0 30rpx;
  height: 88rpx;
  line-height: 88rpx;
  font-family: "PingFangSC-Regular";
  -webkit-justify-content: space-between;
  justify-content: space-between;
}
.weui-cell:before {
  left: 30rpx;
  border-top:1rpx solid #D9D9D9;
}
.weui-cell__bd {
  font-size: 30rpx;
  color: #666666;
}
.weui-cell__ft {
  font-size: 30rpx;
  color: #333333;
}
.weui-cell__ft_in-access:after {
  border-color: #7A6D6E;
  border-width:2rpx 2rpx 0 0
}

.insurance-rental {
  display: -webkit-flex;
  display: flex;
  -webkit-justify-content: center;
  justify-content: center;
  -wenkit-align-items: center;
  align-items: center;
  padding: 40rpx 0;
  background: #fff;
}
.insurance-rental .label {
  font-family: "PingFangSC-Medium";
  font-size: 32rpx;
  color: #333333;
  margin-right: 10rpx;
}
.insurance-rental .value {
  width: 200rpx;
  height: 60rpx;
  background: rgba(237,65,73,0.10);
  border: 1rpx solid rgba(237,65,73,0.30);
  border-radius: 100rpx;  
}
.insurance-rental .value input {
  width: 100%;
  height: 100%;
  border: none;
  background: none;
  font-family: "PingFangSC-Regular";
  font-size: 32rpx;
  color: #ED4149;
  text-align: center;
}
.checkbox_circle {
  width: 32rpx;
  height: 32rpx;
  position: absolute;
  top: 50%;
  left: 30rpx;
  margin-top: -16rpx;
  background: rgba(112,109,110,.2);
  border-radius: 100%;
}
.weui-cell__bd,
.weui-check__label .weui-cell__ft {
  margin-left: 45rpx;
  font-family: "PingFangSC-Regular";
  font-size: 30rpx;
  color: #999999;
}
.weui-cell__bd.checked,
.weui-check__label .weui-cell__ft.checked {
  color: #666;
}
.setbtn {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 98rpx;
  background: #ED4149;
  font-family: "PingFangSC-Regular";
  font-size: 32rpx;
  color: #FFFFFF;
  line-height: 98rpx;
  text-align: center;
  margin-top: 20rpx;
}
.color-gray {
  color: #999 !important;
}

