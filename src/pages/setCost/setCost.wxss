.setcost-wrapper {
  overflow: hidden;
  padding-bottom: 118rpx;
}
.title {
  padding: 0 30rpx;
  height: 88rpx;
  line-height: 88rpx;
  background-color: #EFEFEF;
  font-family: "PingFangSC-Regular";
  font-size: 28rpx;
  color: #888888;
}
.setcost-wrapper .weui-cells:after,
.setcost-wrapper .weui-cells:before {
  border: none;
}
.weui-cell {
  padding: 0 30rpx;
  height: 88rpx;
  line-height: 88rpx;
  font-family: "PingFangSC-Regular";
}
.weui-cell:before {
  left: 30rpx;
  border-top:1rpx solid #D9D9D9;
}
.weui-cell__bd {
  font-size: 30rpx;
  color: #666666;
}
.weui-cell__ft {
  font-size: 30rpx;
  color: #333333;
}
.weui-cell__ft_in-access:after {
  border-color: #7A6D6E;
  border-width:2rpx 2rpx 0 0
}
.setbtn {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 98rpx;
  background: #ED4149;
  font-family: "PingFangSC-Regular";
  font-size: 32rpx;
  color: #FFFFFF;
  line-height: 98rpx;
  text-align: center;
  margin-top: 20rpx;
}