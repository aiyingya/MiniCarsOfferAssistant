.setcost-wrapper {
  overflow: hidden;
  padding-bottom: 118rpx;
}
.title {
  padding: 0 30rpx;
  height: 88rpx;
  line-height: 88rpx;
  background-color: #EFEFEF;
  font-family: "PingFangSC-Regular";
  font-size: 28rpx;
  color: #888888;
}
.setcost-wrapper .weui-cells:after,
.setcost-wrapper .weui-cells:before {
  border: none;
}
.weui-cell {
  padding: 0 30rpx;
  height: 88rpx;
  line-height: 88rpx;
  font-family: "PingFangSC-Regular";
}
.weui-cell:before {
  left: 30rpx;
  border-top:1rpx solid #D9D9D9;
}
.weui-cell__bd {
  font-size: 30rpx;
  color: #666666;
}
.weui-cell__ft {
  font-size: 30rpx;
  color: #333333;
}
.weui-cell__ft_in-access:after {
  border-color: #7A6D6E;
  border-width:2rpx 2rpx 0 0
}
.setbtn {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 98rpx;
  background: #ED4149;
  font-family: "PingFangSC-Regular";
  font-size: 32rpx;
  color: #FFFFFF;
  line-height: 98rpx;
  text-align: center;
  margin-top: 20rpx;
  z-index: 999;
}

.weui-aging{
  width: 100%;
}
.weui-aging label{
  width: 50%;
  display: inline-block;
  margin-bottom:24rpx;
}
.weui-aging label:nth-last-child(1),.weui-aging label:nth-last-child(2){
  margin-bottom:0;
}

.weui-aging label:nth-last-child(1) radio,.weui-aging label:nth-last-child(2) radio{
  margin-top:0;
}

.weui-aging input{
  width:120rpx;
  background:#EFEFEF;
  border:1px solid #CCCCCC;
  border-radius:8rpx;
  overflow:inherit;
  padding-left:10rpx;
  color:#333;
  text-align:center;
  margin-right:8rpx;

}
.weui-aging input,.weui-aging radio,.weui-aging span{
  display:inline-block;
  padding:2rpx 0;
}


.weui-cell-radio{
  padding: 0 30rpx;
}
.weui-aging radio{
  float:left;
  margin-top:4rpx;
}


