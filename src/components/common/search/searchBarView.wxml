<!--page:supplier 顶部搜索栏 -->
<template name="searchBarView">
  <view class="search-bar">
    <view class="search-input">
      <input value="{{ searchBarValue }}" disabled="{{ disabled }}" placeholder-class="search-placeholder" placeholder="{{ searchBarPlaceholder }}" bindinput="onSearchInputEdit"
        bindfocus="onSearchInputFocus" bindconfirm="onSearchInputConfirm" bindblur="onSearchInputBlur"/>
    </view>
  </view>
</template>
