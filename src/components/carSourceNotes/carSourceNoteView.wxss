.car-source-note {
  display: flex;
  justify-content: space-between;
  align-items: center;
  box-sizing: border-box;
  padding: 20rpx 18rpx;

  background: #F6F7F8;
  box-shadow: 0 1rpx 3rpx 0 rgba(0,0,0,0.10);
  border-radius: 8rpx;

  height: 100%;
  width: 100%;
  margin-bottom:10rpx;
}

.car-source-note .left {

}

.car-source-note .right {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: flex-end;
}


.car-source-note-view .text{
  position: relative;
  padding:20rpx 130rpx 20rpx 18rpx;
  background:#F6F7F8;
  box-shadow:0 1rpx 3rpx 0 rgba(0,0,0,0.10);
  border-radius:8rpx;
}

.car-source-note-view .text .box{
  position: absolute;
  right: 0;
  top: 50%;
  margin-top: -50rpx;
  height: 100rpx;
  width: 100rpx;
  border-left: 1px solid  #CCCCCC;
  text-align: center;
}

.car-source-note-view .text .box image{
   height: 40rpx;
   width: 40rpx;
   right: 0;
 }

.car-source-note-view .text .box view:first-child{
  width: 100%;
  height:50rpx;
  padding: 5rpx 0;
}
.car-source-note-view .text .box view:last-child {
  height: 44rpx;
  line-height: 44rpx;
  color: #666666;
  font-size: 22rpx;
}
