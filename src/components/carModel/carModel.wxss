.carModels-cell {
  width: 100%;
  padding: 0 0 40rpx 0;
  background-color: #fff;
  overflow: hidden;
  margin-bottom: 20rpx;
}

.carModels-cell_active {}

.carModels-message {
  position: relative;
  padding: 15rpx 30rpx;
  background: #FAFAFA;
  display: flex;
  -webkit-justify-content: space-between;
  justify-content: space-between;
  -webkit-align-items: center;
  align-items: center;
}

.carModels-message:after {
  content: '';
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  border-left: 8rpx solid #ED4149;
}

.carModels-quote-btn {
  width: auto;
}

.carModels-quote-btn .quotebtn {
  display: block;
  width: 160rpx;
  height: 64rpx;
  line-height: 64rpx;
  background: #ED4149;
  border-radius: 6px;
  color: #fff;
  font-size: 24rpx;
  border: 0;
}

.carModels-quote-btn .quotebtn-active {
  background: rgba(237, 65, 73, .8);
}

.carModels-message-title {
  font-family: "PingFangSC-Medium";
  font-size: 30rpx;
  color: #333333;
  letter-spacing: 0px;
  line-height: 40rpx;
}

.carModels-info {
  margin: 15rpx 0;
  padding: 0;
  overflow: hidden;
  display: flex;
  -webkit-justify-content: space-around;
  justify-content: space-around;
  -webkit-align-items: center;
  align-items: center;
}

.carModels-info .carModels-charts-time,
.carModels-info .carModels-carcolor {
  position: relative;
  width: 40%;
  display: flex;
  -webkit-justify-content: center;
  justify-content: center;
  -webkit-align-items: center;
  align-items: center;
}

.carModels-info .carModels-market_btn {
  width: 140rpx;
  height: 44rpx;
  line-height: 44rpx;
  background-color: rgba(51, 119, 238, .1);
  border-radius: 100rpx;
  font-family: .PingFangSC-Regular;
  font-size: 24rpx;
  color: #3377EE;
  text-align: center;
  margin-right: 20rpx;
}

.carModels-info .carModels-charts-time .label,
.carModels-info .carModels-carcolor .label {
  color: #999999;
}

.carModels-info .carModels-charts-time:after {
  content: '';
  position: absolute;
  top: 0rpx;
  bottom: 0rpx;
  right: 0;
  border-right: 1rpx solid #cccccc;
}

.carModels-info .carModels-supplier,
.carModels-info .carModels-repertory {
  float: left;
  font-family: "PingFangSC-Regular";
  font-size: 28rpx;
  color: #333333;
  margin-right: 20rpx;
}

.carModels-info .carModels-repertory {
  color: #ED4149;
}

.carModels-info .carModels-repertory.adequate {
  color: #14A47D;
}

.carModels-info .carModels-carcolor {
  float: right;
  overflow: hidden;
}

.carModels-info .carModels-carcolor .carcolor-label,
.carModels-info .carModels-carcolor .carcolor-inner {
  float: left;
}

.carModels-info .carModels-carcolor .carcolor-inner {
  display: flex;
  -webkit-justify-content: space-between;
  justify-content: space-between;
  align-items: center;
  height: 40rpx;
}

.carModels-info .carcolor-inner {
  position: relative;
  padding-right: 35rpx;
}

.carModels-info .bar {
  padding: 0;
  width: 100rpx;
  height: 10rpx;
  background-color: #E4E4E4;
  border-radius: 100rpx;
  margin-left: 10rpx;
}

.carModels-info .bar-inner {
  position: absolute;
  top: 0;
  left: 0;
  width: auto;
  height: 10rpx;
  background-color: #3377EE;
  border-radius: 100rpx;
}

.carModels-info .chartstime-inner {
  height: 48rpx;
  line-height: 48rpx;
  padding: 0 27rpx;
  border: 1rpx solid #333333;
  border-radius: 65rpx;
  font-size: 24rpx;
  color: #333;
  margin-right: 10rpx;
}

.carModels-info .chartstime-inner.selected {
  color: #ED4149;
  border: 1rpx solid #ED4149;
}

.carModels-info .icon-arrows {
  width: 24rpx;
  height: 24rpx;
  position: absolute;
  right: 0;
  top: 50%;
  margin-top: -12rpx;
}

.carcolor-item {
  width: 24rpx;
  height: 24rpx;
  background: #FFFFFF;
  border: 1px solid #878787;
  border-radius: 50%;
  margin-left: 10rpx;
}

.charts-inner {
  margin: 0 auto;
  margin: 10rpx auto 0 auto;
  height: 130px;
  background-color: #fff;
}

.canvas-view {
  height: 80px;
  background: #ECF0F7;
}

.charts-inner .canvas,
.pop-canvas {
  width: 100%;
  height: 120px;
  padding: 0;
}

.popup-select-charts .pop-canvas {
  height: calc(100% - 50rpx);
}

.charts-inner .canvas {
  background: #fafafa;
}

.carModels-nodata {
  font-family: "PingFangSC-Regular";
  font-size: 28rpx;
  color: #888888;
  line-height: 30rpx;
  text-align: center;
  margin-top: 20rpx;
}

.charts-tagMessage {
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  display: flex;
  -webkit-justify-content: space-between;
  justify-content: space-between;
  align-items: center;
  background: #fafafa;
  overflow: hidden;
  padding: 0 30rpx;
  height: 20px;
}

.charts-supply-number {
  margin-top: 10rpx;
  line-height: 20px;
}

.charts-tagMessage-inner {
  display: flex;
  -webkit-justify-content: flex-start;
  justify-content: flex-start;
  margin-top: 10rpx;
  line-height: 20px;
}

.popup-shade {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: rgba(0, 0, 0, .5);
  z-index: 99;
}

.popup-charts-main {
  width: 97.3333%;
  height: 240px;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  background-color: #fff;
  border-radius: 10rpx;
  z-index: 100;
}

.popup-select-charts .popup-charts-main {
  width: 100%;
  height: 100%;
  border-radius: 0rpx;
}

.popup-charts-hd,
.popup-marketcharts-hd {
  position: relative;
  width: 100%;
  height: 100rpx;
  margin: 0 auto;
  font-size: 36rpx;
  text-align: center;
  border-bottom: 1rpx solid #ccc;
  font-weight: 500;
  overflow: hidden;
}

.popup-marketcharts-hd .title {
  line-height: 36rpx;
  margin-top: 17rpx;
}

.popup-marketcharts-hd .text {
  font-size: 24rpx;
}

.popup-charts-hd {
  line-height: 100rpx;
}

.popup-charts-bd {
  width: 100%;
  margin: 20rpx auto;
}

.popup-select-charts .popup-charts-bd {
  height: calc(100% - 100rpx);
  box-sizing: border-box;
}

.close-popup {
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  right: 20rpx;
  top: 50%;
  margin-top: -25rpx;
  display: flex;
  -webkit-justify-content: center;
  justify-content: center;
  -webkit-align-items: center;
  align-items: center;
  z-index: 101;
}

.close-popup image {
  display: block;
  width: 24rpx;
  height: 24rpx;
}

.popup-charts-scale {
  font-size: 24rpx;
  line-height: 50rpx;
  padding: 0 15rpx;
  display: flex;
  -webkit-justify-content: space-between;
  justify-content: space-between;
}

.popup-select-charts .popup-charts-scale {
  height: 50rpx;
}

.popup-charts-scale .tocarsources {
  color: #4183c4;
  text-decoration: underline;
}

.popupSelect-main {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  padding: 30rpx;
  background-color: #fff;
  z-index: 102;
  display: flex;
  -webkit-justify-content: start;
  justify-content: start;
  -webkit-align-items: center;
  align-items: center;
  -webkit-flex-wrap: wrap;
  flex-wrap: wrap;
}

.popup-label {
  height: 48rpx;
  line-height: 48rpx;
  width: 150rpx;
  border: 1rpx solid #333333;
  border-radius: 65rpx;
  font-size: 24rpx;
  color: #333;
  margin-right: 10rpx;
  display: flex;
  -webkit-justify-content: center;
  justify-content: center;
  -webkit-align-items: center;
  align-items: center;
  margin-bottom: 10rpx;
}

.popup-label.selected {
  color: #ED4149;
  border: 1rpx solid #ED4149;
}

.popup-label .text {
  margin-left: 15rpx;
}

.popup-label .carcolor-item {
  width: 22rpx;
  height: 22rpx;
  border: none;
  margin: 0;
}

.carModels-inner .nodata {
  margin-top: 20rpx;
}

.carModels-cell .nodata {
  margin-top: 30px;
  height: 140px;
  display: flex;
  align-items: center;
  -webkit-justify-content: center;
  justify-content: center;
}

.carModels-cell .nodata icon {
  margin-right: 10px;
  width: 50px;
  height: 50px;
}

.carModels-cell .nodata .icon-box__desc {
  margin-top: 20px;
}

.button {
  width: 150rpx;
  background: #ED4149;
  border-radius: 8rpx;
  font-family: "PingFangSC-Regular";
  font-size: 24rpx;
  color: #FFFFFF;
  line-height: 30rpx;
  text-align: center;
  padding: 16rpx 15rpx;
}

.button.market-btn {
  width: 120rpx;
  height: 44rpx;
  padding: 0;
  line-height: 44rpx;
  background-color: rgba(51, 119, 238, .1);
  font-size: 24rpx;
  color: #3377EE;
  border-radius: 100rpx;
}

.charts-labels {
  display: -webkit-flex;
  display: flex;
  -webkit-justify-content: flex-start;
  justify-content: flex-start;
  -webkit-align-items: center;
  align-items: center;
}

.charts-label {
  position: relative;
  font-family: 'PingFangSC-Regular';
  font-size: 20rpx;
  color: #333333;
  height: 40rpx;
  line-height: 40rpx;
  padding-left: 35rpx;
  margin-right: 30rpx;
}

.charts-label .label-icon {
  position: absolute;
  left: 0;
  top: 50%;
  margin-top: -12rpx;
  width: 24rpx;
  height: 24rpx;
  background: #ED4149;
  border-radius: 100%;
}

.label2 .label-icon {
  background: #3377EE;
}

.label3 .label-icon {
  background: #B0CDFF;
}

.charts-label .label-icon.icon-nobg {
  background: #dcdce0;
}

.charts-options {
  box-sizing: border-box;
  margin-right: 16rpx;
  margin-bottom: 8rpx;
  height: 40rpx;
  padding: 8rpx 9rpx;
  background: rgba(228, 228, 228, 0.3);
  border-radius: 4rpx;
  font-family: "PingFangSC-Regular";
  font-size: 22rpx;
  color: #333333;
  letter-spacing: 0;
  line-height: 22rpx;
}

.selected {
  background: rgba(241, 124, 124, 0.3)
}

