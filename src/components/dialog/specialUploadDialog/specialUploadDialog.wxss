.specialUploadDialog .effectiveness {
  margin: 10rpx 40rpx 20rpx 40rpx;
  height: 178rpx;
}
.specialUploadDialog .effectiveness-group {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
}
.specialUploadDialog .effectiveness .radio {
  width: 50%;
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #999999;
  line-height: 24rpx;
  display: flex;
  align-items: center;
  justify-content: flex-start;
}
.specialUploadDialog .effectiveness .radio radio {
  tansform: scale(2, 4);
  color: #999999;
}
.specialUploadDialog .custom-input {
  width: 30%;
  background: #EFEFEF;
  border: 1px solid #CCCCCC;
  border-radius: 8rpx;
  margin-right: 10rpx;
}
.specialUploadDialog .line {
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 50%;
  width: 100%;
}
