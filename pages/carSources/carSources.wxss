@import '../../lib/dialog/reliableDialog.wxss';
/* 供货详情.*/
page {
	height: 100%;
}
.container {
	height: 100%;
	width: 100%;
}
.navbar-quote-popup {
	width: 100%;
}
.navbar-quote-popup .navbar-quote-main {
	position: fixed;
	bottom: 0;
	left: 0;
	right: 0;
	z-index: 100;
}
.navbar-quote-popup .navbar-quote-cell {
	width: 100%;
	height: 88rpx;
	line-height: 88rpx;
	background: #fff;
	position: relative;
	font-family: "PingFangSC-Regular";
	font-size: 32rpx;
	color: #333333;
	text-align: center;
}
.navbar-quote-popup .navbar-quote-cell:after {
	content: '';
	position: absolute;
	left: 0;
	right: 0;
	bottom: 0;
	border-bottom: 1rpx solid #ccc;
}
.navbar-quote-popup .navbar-quote-cell:last-child:after {
	border: none;
}
.navbar-quote-popup .navbar-quote-active {
	background-color: #f2f2f2;
}
.rmendCarFacade-popup-wrap {
	position: absolute;
	top: 88rpx;
	bottom: 0;
	height: auto;
	width: 100%;
	z-index: 9;
}
.rmendCarFacade-popup-mask {
	position: absolute;
	width: 100%;
	top: 0;
	bottom: 0;
	width: 100%;
	background-color: rgba(0,0,0,.4);
	z-index: 1;
}
.rmendCarFacade-inner {
	width: 100%;
	height: auto;
	background-color: #fff;
	position: relative;
	z-index: 10;
	-webkit-transform: translate3d(0, 100rpx, 0);
	transform: translate3d(0, 100rpx, 0) ;
}
.rmendCarFacade-inner .weui-cell {
	height: 104rpx;
	line-height: 104rpx;
	padding: 0 30rpx;
}
.rmendCarFacade-inner .weui-cell:before {
	left: 30rpx;
}
.rmendCarFacade-inner .weui-cell__bd {
	font-family: "PingFangSC-Regular";
	font-size: 30rpx;
	color: #333333;
}
.rmendCarFacade-inner .weui-cell__ft,
.rmendCarFacade-inner .weui-cell__ft image {
	display: block;
	width: 36rpx;
	height: 36rpx;
}
.filter-scroll-view {
	padding: 24rpx 30rpx 24rpx 30rpx;
	height: 96rpx;
	width: 150%;
	-webkit-box-sizing:border-box;
	box-sizing:border-box;
	background-color:#fff;
	display:flex;
	justify-content:flex-start;
	-webkit-align-items:center;
	align-items:center;
	white-space: nowrap;
}
.filter-scroll-view .scroll-filter {
	float:left;
	white-space: nowrap;
	-webkit-box-sizing:border-box;
	box-sizing:border-box;
	display:flex;
	justify-content:flex-start;
	-webkit-align-items:center;
	align-items:center;
	position:relative;
	padding: 0 27rpx;
	z-index: 1;
}
.filter-scroll-view .scroll-filter:last-child {
	padding: 0 0 0 27rpx;
}
.filter-scroll-view .scroll-filter:first-child {
	padding: 0 27rpx 0 0;
}
.filter-scroll-view .scroll-filter:after {
	content: '';
	position: absolute;
	top: 4rpx;
	bottom: 4rpx;
	right: 0;
	left: 0;
	border-right: 1rpx solid #CCCCCC;
	z-index: -1;
}
.filter-scroll-view .scroll-filter:last-child:after {
	border-right: 1rpx solid #FFFFFF;
}
.filter-scroll-view .scroll-filter .filter {
	padding: 12rpx 27rpx 12rpx 27rpx;
	border:1rpx solid #333333;
	border-radius:65rpx;
	font-size:24rpx;
	margin-right:10rpx;
	display:inline-block;
	font-family: "PingFangSC-Regular";
	letter-spacing: 0;
	line-height: 24rpx;
}
.filter-scroll-view .scroll-filter .filter:last-child {
	margin-right: 0rpx;
}
.filter-scroll-view .scroll-filter .selected {
	border: 1rpx solid #ED4149;
	color: #ED4149;
}
.filter-scroll-view .scroll-filter .unselected {
	border: 1rpx solid #333333;
	color: #333333;
}
.carSources-topbar {
	height: 104rpx;
	background-color: #fff;
	overflow: hidden;
}
.carSources-topbar .weui-media-box {
	padding: 0 24rpx;
}
.carSources-topbar .weui-media-box__hd,
.carSources-topbar .weui-media-box__thumb {
	width: 150rpx;
	height: 100rpx;
}
.carSources-topbar .weui-media-box__title {
	width: 100%;
	height: 40rpx;
	line-height: 40rpx;
	font-family: "PingFangSC-Medium";
	font-size: 30rpx;
	color: #333333;
	margin-top: 10rpx;
}
.carSources-topbar .weui-media-box__desc {
	font-family: "PingFangSC-Regular";
	font-size: 26rpx;
	color: #999999;
	letter-spacing: 0rpx;
	line-height: 26rpx;
}
.carSources-body {
	width: 100%;
	margin-top: 20rpx;
	position: relative;
	min-height: -webkit-calc(100% - 180rpx);
	min-height: calc(100% - 180rpx);
	overflow: hidden;
}
.carSources-navbar {
	width: 100%;
	height: 88rpx;
	display: -webkit-flex;
	display: flex;
	-webkit-justify-content: space-between;
	justify-content: space-between;
	-webkit-align-items: center;
	align-items: center;
	background-color: #fff;
	position: relative;
	z-index: 10;
}
.carSources-navbar:after,
.carSources-cell:after {
	content: '';
	position: absolute;
	left: 0;
	right: 0;
	bottom: 0;
	border-bottom: 1rpx solid #E4E4E4;
}
.carSources-navbar .navbar {
	width: 20%;
	position: relative;
	overflow: visible;
	font-family: "PingFangSC-Regular";
	font-size: 28rpx;
	color: #666666;
	text-align: center;
}
.carSources-navbar .cars-facade {
	width: 50%;
}
.carSources-navbar .cars-facade:after {
	content: '';
	position: absolute;
	top: -6rpx;
	bottom: -6rpx;
	right: 0;
	border-right: 1rpx solid #E4E4E4;
}
.carSources-navbar .navbar-text {
	position: relative;
	display: inline-block;
	padding-right: 30rpx;
	font-family: "PingFangSC-Regular";
	font-size: 28rpx;
	color: #666666;
	letter-spacing: 0rpx;
	line-height: 28rpx;
}
.carSources-navbar .icon-dropdownarrow {
	width: 16rpx;
	height: 16rpx;
	position: absolute;
	top: 50%;
	right: 0;
	margin-top: -8rpx;
}
.carSources-navbar .navbar .navbar-btn {
	display: block;
	width: 100rpx;
	height: 48rpx;
	line-height: 48rpx;
	background-color: #fff;
	border: 1rpx solid #CCCCCC;
	border-radius: 64rpx;
	font-family: "PingFangSC-Regular";
	font-size: 26rpx;
	color: #666666;
	text-align: center;
	margin: 0 auto;
}
.carSources-navbar .navbar .navbar-btn.in_stock {
	border: 1px solid #ED4149;
	color: #ED4149;
}

/* 供货 SKU 分区样式 */
