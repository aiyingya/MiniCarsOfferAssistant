}
.carSources-cell .carSources-cell-content .logistics-list {
	display: flex;
	flex-wrap: wrap;
	justify-content: space-between;
}
.carSources-cell .carSources-cell-content .logistics-list .logistics-block {
	display: flex;
	flex-direction: column;
	justify-content: center;
	align-items: center;
	padding: 8rpx 19rpx;

	border: 1rpx solid #B2B2B2;
	border-radius: 8rpx;
	font-family: "PingFangSC-Regular";
	font-size: 22rpx;
	color: #888888;
	letter-spacing: 0rpx;
	line-height: 22rpx;
	margin-right: 13rpx;
}
.carSources-cell .carSources-cell-content .logistics-list .selected {
	border: 1rpx solid #3377EE;
	border-radius: 8rpx;
	font-family: "PingFangSC-Regular";
	font-size: 22rpx;
	color: #3377EE;
	letter-spacing: 0rpx;
	line-height: 22rpx;
}
/* 提示信息 */
.fivth-part {
	display: flex;
	align-items: baseline;
}
/* 底部按钮 */
.sixth-part {
	display: flex;
	justify-content: space-between;
}
.carSources-cell .carSources-cell-content .button {
	height: 64rpx;
	line-height: 64rpx;
	text-align: center;
	width: 48.5%;
	font-family: "PingFangSC-Regular";
	font-size: 28rpx;
	border-radius: 8rpx;
    -webkit-box-sizing: border-box;
	box-sizing: border-box;
}
.carSources-cell .carSources-cell-content .full {
	width: 100%;
}
.carSources-cell .carSources-cell-content .confirm {
	background: #ED4149;
	color: #FFFFFF;
}
.carSources-cell .carSources-cell-content .normal {
	border: 1rpx solid #ED4149;
	color: #ED4149;
}

/* 点 前缀*/
.icon-point-padding {
	padding-left: 24rpx;
}
.icon-point {
	position: relative;
	padding-left: 24rpx;
}
.icon-point:before {
	position: absolute;
	content: '';
	width: 8rpx;
	height: 8rpx;
	background: #999999;
	-moz-border-radius: 4rpx;
	-webkit-border-radius: 4rpx;
	border-radius: 4rpx;
	left: 0;
	top: 50%;
	margin-top: -4rpx;
}

.icon-small-point {
	position: relative;
	padding-left: 23rpx;
}
.icon-small-point:before {
	position: absolute;
	content: '';
	width: 6rpx;
	height: 6rpx;
	background: #999999;
	-moz-border-radius: 3rpx;
	-webkit-border-radius: 3rpx;
	border-radius: 3rpx;
	left: 9rpx;
	top: 50%;
	margin-top: -3rpx;
}
/* 虚线 */
.temp {
	position: relative;
	padding-top: 25rpx;
	margin-top: 25rpx;
	padding-bottom: 20rpx;
	margin-bottom: 20rpx;
}
.temp:before {
	content: '';
	position: absolute;
	top: 0;
	left: 0;
	right: 0;
	border-top: 1rpx dotted #D9D9D9;
}
.temp:after {
	content: '';
	position: absolute;
	bottom: 0;
	left: 0;
	right: 0;
	border-bottom: 1rpx dotted #D9D9D9;
}
