}
.carSources-section .carSources-section-content .carSources-section-price .discount {
	font-family: "PingFangSC-Regular";
	font-size: 26rpx;
	color: #333333;
	line-height: 26rpx;
	display: inline;
}
.carSources-section .carSources-section-content .carSources-section-price .price {
	display: inline;
	font-family: "SFUIDisplay-Regular";
	font-size: 34rpx;
