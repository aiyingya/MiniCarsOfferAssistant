<!--index.wxml-->
<view class="container">
  <!-- 搜索车型.-->
  <view class="search-wrap">
  	<view class="search-inner">
  		<view class="search-icon">
  			<image class="search-img" src="../../images/icons/icon_find_line_black@3x.png"></image>
  		</view>
			<input 
				bindtap="searchCarType" 
				class="search-input" 
				placeholder-class="search-placeholder"
				disabled 
				placeholder="输入指导价/车款名" 
				/>
  	</view>
  </view>
  
  <!-- 热门品牌.-->
  <view class="hot-carsbrand-wrap">
  	<view class="hot-carsbrand-title">
  		<view class="hot-carsbrand-text">热门品牌</view>
  	</view>
  
  	<view class="hot-carsbrand-stand">
  		<view class="stand-item">
  			<image mode="aspectFit" class="hot-carslogo" src="../../images/carlogo/car-1.png"></image>
  		</view>
  		<view class="stand-item">
  			<image mode="aspectFit" class="hot-carslogo" src="../../images/carlogo/car-1.png"></image>
  		</view>
  		<view class="stand-item">
  			<image mode="aspectFit" class="hot-carslogo" src="../../images/carlogo/car-1.png"></image>
  		</view>
  		<view class="stand-item">
  			<image mode="aspectFit" class="hot-carslogo" src="../../images/carlogo/car-1.png"></image>
  		</view>
  		<view class="stand-item">
  			<image mode="aspectFit" class="hot-carslogo" src="../../images/carlogo/car-1.png"></image>
  		</view>
  	</view>	
  </view>
  
  <!-- cars list.-->
  <view class="viewCarsList-wrap">
  	<view class="viewCarsList-calls">
  		<view class="viewCarsList-index">A</view>
  		<view class="weui-cells weui-cells_in-small-appmsg">
				<navigator url="" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
					<view class="weui-cell__hd">
						<image src="../../images/carlogo/car-1.png" class="viewCarsList_icon-index" />
					</view>
					<view class="weui-cell__bd weui-cell_primary">
							<view>文字标题</view>
					</view>
					
				</navigator>
				<navigator url="" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
					<view class="weui-cell__hd"><image src="{{icon20}}" style="width: 20px;height: 20px;margin-right: 5px" /></view>
					<view class="weui-cell__bd weui-cell_primary">
							<view>文字标题</view>
					</view>
					<view class="weui-cell__ft weui-cell__ft_in-access"></view>
				</navigator>
			</view>
  	</view>
  </view>
</view>
