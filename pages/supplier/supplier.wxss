.supplier-cells {
	margin-top: 10rpx;
}
.supplier-cell {
	width: 100%;
	height: 100rpx;
	line-height: 100rpx;
	background-color: #fff;
	padding: 0 30rpx;
	-webkit-box-sizing: border-box;
  box-sizing: border-box; 
	display: flex;
	-webkit-justify-content: space-between;
	justify-content: space-between;
}
.supplier-name {
	font-family: "PingFangSC-Medium";
	font-size: 30rpx;
	color: #333333;
}
.supplier-source {
	font-family: "PingFangSC-Regular";
	font-size: 26rpx;
	color: #385C9A;
}
.supplier-source .label {
	color: #666;
	display: inline;
}