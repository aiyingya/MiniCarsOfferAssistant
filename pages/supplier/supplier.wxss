.container {
	width: 100%;
	overflow: hidden;
}
.supplier-cells {
	width: 100%;
	margin-top: 10rpx;
	overflow: hidden;
}
.supplier-cell {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100rpx;
	line-height: 100rpx;
	background-color: #fff;
	padding: 0 30rpx;
	-webkit-box-sizing: border-box;
  box-sizing: border-box; 
	display: flex;
	-webkit-justify-content: space-between;
	justify-content: space-between;
	z-index: 9;
	transition: left 0.2s ease-in-out;
}
.supplier-name {
	font-family: "PingFangSC-Medium";
	font-size: 30rpx;
	color: #333333;
}
.supplier-source {
	font-family: "PingFangSC-Regular";
	font-size: 26rpx;
	color: #385C9A;
}
.supplier-source .label {
	color: #666;
	display: inline;
}
.supplier-cell-inner {
	width: 100%;
	position: relative;
	overflow: hidden;
	height: 100rpx;
}
.phone-contact,
.cancel-attention {
	position: absolute;
	top: 0;
	right: 0;
	width: 100rpx;
	height: 100rpx;
	background: #ED4149;
	font-size: 32rpx;
	color: #FFFFFF;
	text-align: center;
	line-height: 100rpx;
	z-index: 1;
}
.phone-contact {
	width: 200rpx;
	background-color: #c8c7cd;
	right: 100rpx;
}
@-webkit-keyframes bounceInRight {
  from, 60%, 75%, 90%, to {
    -webkit-animation-timing-function: cubic-bezier(0.215, 0.610, 0.355, 1.000);
    animation-timing-function: cubic-bezier(0.215, 0.610, 0.355, 1.000);
  }

  from {
    opacity: 0;
    -webkit-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
  }

  60% {
    opacity: 1;
    -webkit-transform: translate3d(-5px, 0, 0);
    transform: translate3d(-5px, 0, 0);
  }

  75% {
    -webkit-transform: translate3d(-40px, 0, 0);
    transform: translate3d(-40px, 0, 0);
  }

  90% {
    -webkit-transform: translate3d(-80px, 0, 0);
    transform: translate3d(-80px, 0, 0);
  }

  to {
    -webkit-transform: translate3d(-100px, 0, 0);
    transform: translate3d(-100px, 0, 0);
  }
}

@keyframes bounceInRight {
  from, 60%, 75%, 90%, to {
    -webkit-animation-timing-function: cubic-bezier(0.215, 0.610, 0.355, 1.000);
    animation-timing-function: cubic-bezier(0.215, 0.610, 0.355, 1.000);
  }

  from {
    opacity: 0;
    -webkit-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
  }

  60% {
    opacity: 1;
    -webkit-transform: translate3d(-5px, 0, 0);
    transform: translate3d(-5px, 0, 0);
  }

  75% {
    -webkit-transform: translate3d(-40px, 0, 0);
    transform: translate3d(-40px, 0, 0);
  }

  90% {
    -webkit-transform: translate3d(-80px, 0, 0);
    transform: translate3d(-80px, 0, 0);
  }

  to {
    -webkit-transform: translate3d(-100px, 0, 0);
    transform: translate3d(-100px, 0, 0);
  }
}

.bounceInRight {
  -webkit-animation: bounceInRight 500ms ease 1 forwards ;
	animation: bounceInRight 500ms ease 1 forwards ;
	-webkit-animation-play-state: running;
}