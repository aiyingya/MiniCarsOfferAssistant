{
  "presets": [
    "env",
    "stage-0",
    "flow"
  ],
  "plugins": [
    "transform-runtime"
  ],
  "ignore": [
    "node_modules/babel-runtime/**/*.js",
    "node_modules/core-js/**/*.js",
    "node_modules/regenerator-runtime/**/*.js",
    "node_modules/lodash/**/*.js",
    "node_modules/lodash.*/**/*.js"
  ]
}
